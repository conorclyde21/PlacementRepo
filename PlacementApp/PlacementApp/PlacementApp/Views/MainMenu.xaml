<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PlacementApp.Pages.MainMenu" 
             NavigationPage.HasNavigationBar="false">
    
    <ContentPage.Content>
        <StackLayout Spacing="20">

            <Button Text="Profile" Clicked="MenuToProfile"/>
            <Button Text="Placement" Clicked="MenuToPlacement"/>
            <Button Text="Assessment" Clicked="MenuToAssessment"/>
            <Button Text="Reflection Log" Clicked="MenuToReflect"/>
            <Button Text="Log Out" Clicked="MenuToLogin"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>